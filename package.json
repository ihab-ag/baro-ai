{
  "name": "baro-ai",
  "version": "1.0.0",
  "description": "Telegram AI Accounting Bot with LangChain",
  "main": "dist/telegram/bot.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/telegram/bot.js",
    "pretest": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:preflight": "tsx tests/test-runner.ts",
    "prestart": "npm run test:preflight",
    "dev": "tsx src/telegram/bot.ts",
    "telegram": "tsx src/telegram/bot.ts"
  },
  "keywords": [
    "langchain",
    "ai",
    "telegram",
    "bot",
    "accounting",
    "expense-tracker"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@langchain/core": "^1.0.0",
    "@langchain/groq": "^1.0.0",
    "@langchain/openai": "^1.0.0",
    "@supabase/supabase-js": "^2.76.1",
    "dotenv": "^16.4.5",
    "langchain": "^1.0.0",
    "node-telegram-bot-api": "^0.66.0",
    "openai": "^4.70.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/node-telegram-bot-api": "^0.64.12",
    "@vitest/coverage-v8": "^4.0.5",
    "@vitest/ui": "^4.0.5",
    "tsx": "^4.19.0",
    "typescript": "^5.5.0",
    "vitest": "^4.0.5"
  }
}
